{
  "name": "hhsb-apollo-grapql-api",
  "version": "0.1.0",
  "main": "app/server.js",
  "description": "Scoreboard Graphql API",
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "scripts": {
    "start": "pm2-runtime start app/ecosystem.config.js --env production",
    "dev": "SET NODE_ENV=dev&&nodemon app/server.js --exec babel-node --presets=@babel/preset-env",
    "dev-mac": "cross-env NODE_ENV=dev nodemon app/server.js --exec babel-node --presets=@babel/preset-env",
    "build": "rimraf dist/ && babel ./app/ --out-dir dist/ --ignore ./node_modules,./app/tests,./.babelrc,./npm-debug.log",
    "build-deploy": "npm run build && pm2 delete all && pm2 start app/ecosystem.config.js",
    "unittest": "env-cmd ./.env mocha --exit app/tests --recursive --require @babel/register --require @babel/polyfill --check-leaks",
    "test": "jest",
    "lint": "eslint .",
    "dev-db-start": "./dev-db start",
    "dev-db-stop": "./dev-db stop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scoreboardinc/voxpop-api"
  },
  "keywords": [
    "Node.js",
    "Javascript",
    "GraphQL",
    "Express",
    "Apollo"
  ],
  "author": "Oliver Molina <olivermolina10@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scoreboardinc/voxpop-api/issues"
  },
  "homepage": "hhttps://github.com/scoreboardinc/voxpop-api",
  "dependencies": {
    "@babel/register": "^7.9.0",
    "@graphql-tools/schema": "^10.0.0",
    "@sendgrid/mail": "^8.1.5",
    "apollo-server-express": "^3.12.1",
    "axios": "^0.21.1",
    "babel-core": "^6.26.3",
    "babel-plugin-root-import": "^6.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "chai": "^4.2.0",
    "cors": "^2.8.3",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "express": "^4.16.3",
    "graphql": "^16.0.0",
    "graphql-server-express": "^1.3.2",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.1",
    "http": "0.0.1-security",
    "ioredis": "^5.8.0",
    "jest": "^26.0.1",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.15",
    "lyricist": "^2.2.2",
    "mocha": "^7.1.2",
    "moment-timezone": "^0.5.29",
    "mongodb": "^3.5.7",
    "mongoose": "^5.9.13",
    "nanoid": "^3.1.23",
    "node-url-shortener": "^1.0.1",
    "nodemailer": "^6.4.11",
    "path": "^0.12.7",
    "pm2": "^4.4.0",
    "promise": "^8.0.1",
    "prop-types": "^15.6.2",
    "rate-limiter-flexible": "^8.0.1",
    "request": "^2.81.0",
    "sinon": "^9.0.2",
    "stripe": "^8.89.0",
    "subscriptions-transport-ws": "^0.9.4",
    "underscore": "^1.8.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.0",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  },
  "peerDependencies": {
    "graphql": "^16.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
